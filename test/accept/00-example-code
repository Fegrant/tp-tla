Graph A

add to A:
    nodes b, c, d, e, f
    edges c-4-d, b-2-c

remove from A:
    edges c-4-d
    nodes b	/* Internamente tambiÃ©n borra la arista b-c */

apply to A:
    bfs from c to d > hola
    dfs from d to c > archivoA
    colors:
        #ff0000 e, d		/* Es lo mismo que red e, d */
        #00ff00 c, f		/* Es lo mismo que green c, f */
    find cut nodes
    delete cut nodes > adios
